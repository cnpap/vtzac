{
  "name": "vtzac",
  "type": "module",
  "version": "0.0.2-beta.3",
  "packageManager": "pnpm@10.17.1",
  "description": "_description_",
  "author": "cnpap",
  "license": "MIT",
  "funding": "https://github.com/sponsors/cnpap",
  "homepage": "https://github.com/cnpap/vtzac#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnpap/vtzac.git"
  },
  "bugs": "https://github.com/cnpap/vtzac/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./hook": "./dist/hook.js",
    "./fetch": "./dist/fetch.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "dev:backend": "tsx watch --tsconfig tsconfig.server.json src/backend/main.ts",
    "dev:html": "live-server src/backend/case --port=5173",
    "format": "prettier --write \"src/backend/case/**/*.html\" \"docs/**/*.md\" \"docs/**/*.css\"",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:ci": "concurrently --kill-others --success first \"tsx --tsconfig tsconfig.server.json src/backend/main.ts\" \"wait-on http://localhost:3001/api/hello && vitest run\"",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0",
    "@nestjs/platform-express": ">=9.0.0",
    "multer": ">=2.0.2",
    "ofetch": ">=1.4.1",
    "reflect-metadata": ">=0.1.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@nestjs/swagger": "catalog:",
    "fast-glob": "catalog:",
    "ofetch": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@nestjs/cli": "catalog:",
    "@nestjs/common": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@types/express": "catalog:",
    "@types/multer": "catalog:",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "concurrently": "catalog:",
    "eslint": "catalog:cli",
    "globals": "catalog:",
    "lint-staged": "catalog:cli",
    "live-server": "catalog:",
    "multer": "catalog:",
    "prettier": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:testing",
    "tsdown": "catalog:cli",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "vite": "catalog:cli",
    "vitepress": "catalog:",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "wait-on": "catalog:",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "playground/**/*.{ts,tsx,js,jsx,json,css,md}": "prettier --write",
    "!(playground/**)": "eslint --fix",
    "playground/**/*.{ts,tsx,js,jsx}": "eslint -c playground/eslint.config.js --fix"
  }
}
