{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "pnpm --filter nestjs-example start",
    "build": "pnpm build:frontend && pnpm build:backend",
    "build:frontend": "tsc -b && vite build",
    "build:backend": "nest build",
    "start": "concurrently \"pnpm preview\" \"pnpm start:backend\"",
    "start:backend": "node dist/backend/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@nestjs/common": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "@tailwindcss/vite": "catalog:",
    "@types/multer": "catalog:",
    "antd": "catalog:",
    "nestjs-example": "workspace:*",
    "multer": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-use": "^17.6.0",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@nestjs/cli": "catalog:",
    "@types/node": "catalog:types",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:cli",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "typescript-eslint": "catalog:",
    "vite": "catalog:cli",
    "vite-plugin-inspect": "^11.3.3",
    "vtzac": "link:../"
  }
}
